= form_for @result do |f|
  - if @result.errors.any?
    #error_explanation
      %h2= "#{pluralize(@result.errors.count, "error")} prohibited this result from being saved:"
      %ul
        - @result.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :total
    = f.number_field :total
  .field
    = f.label :user
    = f.text_field :user
  .field
    = f.label :tempuser
    = f.text_field :tempuser
  .field
    = f.label :round
    = f.text_field :round
  .actions
    = f.submit 'Save'
